@model IEnumerable<EmployeeReview.Controllers.Title>

@{
    ViewBag.Title = "Create New Title";
}

<h2>Create New Title</h2>

<p>
    @Html.ActionLink("Create New", "Create") |
    @Html.ActionLink("Home", "../Home/Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TitleDept)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TitleDept)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
